{
  "name": "multiplayer-pacman",
  "version": "0.0.1",
  "description": "Multiplayer pacman game. More information coming soon.",
  "main": "src/backend/Server.js",
  "scripts": {
    "dev": "nodemon src/backend/Server.js & npm run tailwind:dev & wait",
    "start": "node src/backend/Server.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tailwind": "npx tailwindcss -c ./tailwind.config.cjs -i ./src/frontend/main.css -o ./src/frontend/styles.css",
    "tailwind:dev": "npx tailwindcss -c ./tailwind.config.cjs -i ./src/frontend/main.css -o ./src/frontend/styles.css --watch",
    "tailwind:prod": "npx tailwindcss -c ./tailwind.config.cjs -i ./src/frontend/main.css -o ./src/frontend/styles.css --minify",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristianLisle/multiplayer-pacman.git"
  },
  "keywords": [
    "PacMan",
    "Multiplayer",
    "Multi-Player",
    "Pac-Man",
    "multiplayer",
    "multi-player"
  ],
  "author": "Christian Lisle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChristianLisle/multiplayer-pacman/issues"
  },
  "homepage": "https://github.com/ChristianLisle/multiplayer-pacman#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "babel-cli": "^6.26.0",
    "babel-jest": "^27.5.1",
    "babel-preset-env": "^1.7.0",
    "chance": "^1.1.8",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "superagent": "^7.1.2",
    "supertest": "^6.2.2",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {
    "express": "^4.17.3",
    "socket.io": "^4.5.1"
  },
  "type": "module",
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
